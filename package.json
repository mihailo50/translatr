{
  "name": "translatr",
  "version": "0.1.0",
  "private": true,
  "browserslist": {
    "production": [
      "defaults",
      "not IE 11",
      "not dead",
      "> 0.2%",
      "last 2 versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "dev:secure": "node server.js",
    "dev:http": "next dev -H 0.0.0.0",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "quality": "npm run lint && npm run format:check && npm run type-check && npm run knip",
    "prepare": "husky"
  },
  "dependencies": {
    "@livekit/components-react": "^2.9.17",
    "@livekit/components-styles": "^1.2.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.87.3",
    "livekit-client": "^2.16.1",
    "livekit-server-sdk": "^2.15.0",
    "lucide-react": "^0.561.0",
    "next": "16.0.10",
    "openai": "^6.13.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "sonner": "^2.0.7",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.10",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "knip": "^5.82.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "selfsigned": "^5.4.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
